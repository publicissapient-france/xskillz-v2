{
    "name": "skillz",
    "version": "2.0.0",
    "description": "Skillz application",
    "private": true,
    "scripts": {
        "start": "node bin/server.js",
        "test": "./node_modules/mocha/bin/mocha src/test --recursive --compilers js:babel-core/register,scss:./src/test/null-compiler --require src/test/setup.js",
        "test:watch": "./node_modules/mocha/bin/mocha src/test --recursive -watch --compilers js:babel-core/register,scss:./src/test/null-compiler --require src/test/setup.js",
        "clean": "rimraf dist",
        "build": "webpack --progress --verbose --colors --display-error-details --config webpack/common.config.js",
        "build:production": "npm run clean && NODE_ENV=production npm run build",
        "ci-test": "./node_modules/mocha/bin/mocha src/test --recursive --reporter mochawesome --compilers js:babel-core/register,scss:./src/test/null-compiler --require src/test/setup.js && ./node_modules/istanbul/lib/cli.js --include-all-sources cover ./node_modules/mocha/bin/_mocha -x \"mochawesome-reports/**\" -x \"scripts/**\" -x \"bin/**\" -x \"**.conf**.js\" -x \"dist/**\" -- -R spec src/test --recursive --compilers js:babel-core/register,scss:./src/test/null-compiler --require src/test/setup.js && ./node_modules/istanbul/lib/cli.js report clover",
        "ci-lint": "./node_modules/eslint/bin/eslint.js -f checkstyle src/** > reports/checkstyle-report.xml ; exit 0",
        "deploy": "node ./scripts/deploy-surge.js",
        "connect": "sudo docker exec -i -t xskillzv2_web_1 /bin/bash",
        "docker:build": "npm run build:production && docker build -t xebiafrance/skillz-web .",
        "docker:push": "docker push xebiafrance/skillz-web",
        "docker:build:http": "npm run build:production && docker build -t xebiafrance/skillz-web-http .",
        "docker:push:http": "docker push xebiafrance/skillz-web-http"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/xebia-france/xskillz-v2"
    },
    "homepage": "",
    "bugs": "https://github.com/xebia-france/xskillz-v2/issues",
    "keywords": [
        "react",
        "redux",
        "es6",
        "babel",
        "redux",
        "webpack"
    ],
    "author": "Benjamin Lacroix <blacroix@xebia.fr>",
    "devDependencies": {
        "autoprefixer": "^6.3.7",
        "babel-cli": "^6.5.1",
        "babel-core": "^6.3.26",
        "babel-eslint": "^7.1.1",
        "babel-loader": "^6.2.0",
        "babel-plugin-react-transform": "^2.0.0",
        "babel-plugin-transform-decorators-legacy": "^1.3.4",
        "babel-polyfill": "^6.3.14",
        "babel-preset-es2015": "^6.3.13",
        "babel-preset-react": "^6.3.13",
        "babel-preset-react-hmre": "^1.0.1",
        "babel-preset-stage-0": "^6.3.13",
        "bourbon": "^4.3.4",
        "bourbon-neat": "^1.9.1",
        "classnames": "^2.1.3",
        "css-loader": "^0.26.0",
        "css-modules-require-hook": "^4.0.1",
        "diacritics": "^1.2.3",
        "eslint": "^3.0.0",
        "eslint-config-airbnb": "^13.0.0",
        "eslint-plugin-import": "^2.2.0",
        "eslint-plugin-jsx-a11y": "^3.0.1",
        "eslint-plugin-react": "^6.7.1",
        "expect": "^1.12.0",
        "exports-loader": "^0.6.2",
        "express": "^4.13.3",
        "express-open-in-editor": "^1.0.0",
        "extract-text-webpack-plugin": "^1.0.1",
        "file-loader": "^0.9.0",
        "file-saver": "^1.3.3",
        "fs-extra": "^1.0.0",
        "generate-file-plugin": "0.1.2",
        "history": "^4.4.0",
        "immutable": "^3.8.1",
        "imports-loader": "^0.6.4",
        "intl": "1.2.5",
        "intl-locales-supported": "1.0.0",
        "isomorphic-fetch": "^2.2.1",
        "istanbul": "^1.1.0-alpha.1",
        "jsdom": "^9.4.0",
        "less": "^2.7.1",
        "less-loader": "^2.2.3",
        "lodash": "^4.17.2",
        "material-ui": "^0.16.4",
        "mime": "^1.3.4",
        "mocha": "^3.1.2",
        "mochawesome": "^1.3.1",
        "moment": "^2.29.4",
        "morgan": "^1.6.1",
        "npm-check": "^5.2.2",
        "postcss-import": "^8.1.2",
        "postcss-loader": "^1.1.1",
        "react": "^15.2.0",
        "react-addons-css-transition-group": "^15.2.0",
        "react-addons-shallow-compare": "^15.4.0",
        "react-addons-test-utils": "^15.2.0",
        "react-document-meta": "^2.0.0-rc2",
        "react-dom": "^15.2.0",
        "react-geosuggest": "2.0.0",
        "react-hot-loader": "1.3.1",
        "react-infinite": "^0.10.0",
        "react-loading-order-with-animation": "^1.0.0",
        "react-redux": "^4.0.0",
        "react-router": "^3.0.0",
        "react-router-redux": "^4.0.5",
        "react-tap-event-plugin": "^2.0.1",
        "react-transform-hmr": "^1.0.1",
        "recursive-readdir": "^2.0.0",
        "redux": "^3.0.4",
        "redux-form": "^6.2.0",
        "redux-logger": "^2.6.1",
        "redux-thunk": "^2.1.0",
        "resolve-url-loader": "^1.4.3",
        "rimraf": "^2.5.3",
        "sass-loader": "^4.1.1",
        "store": "^1.3.20",
        "style-loader": "^0.13.0",
        "surge": "^0.18.0",
        "url-loader": "^0.5.6",
        "validator": "6.1.0",
        "webpack": "^1.9.6",
        "webpack-dev-middleware": "^1.2.0",
        "webpack-dev-server": "^1.8.2",
        "webpack-hot-middleware": "^2.4.1",
        "webpack-merge": "^0.17.0"
    }
}
