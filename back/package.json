{
    "name": "skillz",
    "version": "2.0.0",
    "description": "",
    "main": "src/index.js",
    "scripts": {
        "start": "pm2 start configuration.json",
        "dev": "RDS_USER=skillz RDS_PASSWORD=skillz RDS_DATABASE=skillz RDS_HOST=localhost NODE_ENV=development ./node_modules/nodemon/bin/nodemon.js src/index.js",
        "lint": "eslint src tests",
        "migration:new": "db-migrate create --config database/database.json --migrations-dir database/migrations",
        "migration:up": "db-migrate up --config database/database.json --migrations-dir database/migrations",
        "test:migration:reset": "db-migrate reset --env tests --config database/database.json --migrations-dir database/migrations",
        "test:migration:up": "db-migrate --env tests up --config database/database.json --migrations-dir database/migrations",
        "pretest": "npm run lint && npm run test:migration:up",
        "test": "PORT=8081 RDS_HOST='localhost' RDS_PORT=3307 RDS_DATABASE=skillz RDS_USER=skillz RDS_PASSWORD=skillz ./node_modules/istanbul/lib/cli.js --include-all-sources cover ./node_modules/mocha/bin/_mocha -- --timeout 10000 --recursive -R spec tests",
        "posttest": "./node_modules/istanbul/lib/cli.js report clover",
        "integration:migration:reset": "db-migrate reset --env itests --config database/database.json --migrations-dir database/migrations",
        "integration:migration:up": "db-migrate --env itests up --config database/database.json --migrations-dir database/migrations",
        "imigrate": "npm run integration:migration:reset && npm run integration:migration:up",
        "preitest": "npm run lint && npm run imigrate",
        "itest": "PORT=8081 RDS_HOST='skillztests.c3fhpdl9pmsv.eu-central-1.rds.amazonaws.com' RDS_PORT=3306 RDS_DATABASE=skillztests RDS_USER=skillztests RDS_PASSWORD=skillztests ./node_modules/istanbul/lib/cli.js --include-all-sources cover ./node_modules/mocha/bin/_mocha --report lcovonly -- --timeout 10000 --recursive -R spec tests && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
        "postitest": "npm run posttest",
        "dump": "/Applications/MAMP/Library/bin/mysqldump --host=\"ec2-54-229-205-2.eu-west-1.compute.amazonaws.com\" skillz -u skillz -p  > dump.sql"
    },
    "pre-commit": [
        "test"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "bcrypt": "1.0.3",
        "bcrypt-then": "1.1.0",
        "bluebird": "3.5.0",
        "body-parser": "1.18.2",
        "cors": "2.8.4",
        "csv": "1.1.1",
        "es6-promise": "4.1.1",
        "express": "4.16.1",
        "gravatar": "1.6.0",
        "isomorphic-fetch": "2.2.1",
        "json2csv": "^3.11.2",
        "mailgun-js": "0.13.1",
        "moment": "2.18.1",
        "mysql-promise": "4.1.0",
        "nodemailer": "6.4.16",
        "nodemailer-ses-transport": "1.5.0",
        "pm2": "2.7.1",
        "promise": "8.0.1",
        "promise-mysql": "3.1.0",
        "raven": "^2.1.2",
        "uuid": "3.1.0",
        "winston": "2.3.1"
    },
    "devDependencies": {
        "chai": "4.1.2",
        "coveralls": "3.0.0",
        "db-migrate": "0.10.0-beta.24",
        "db-migrate-mysql": "1.1.10",
        "eslint": "4.7.1",
        "istanbul": "0.4.5",
        "mocha": "3.5.3",
        "mocha-lcov-reporter": "1.3.0",
        "nodemon": "1.12.1",
        "npm-check": "5.4.5",
        "pre-commit": "1.2.2",
        "sinon": "4.0.0",
        "superagent-promise": "1.1.0",
        "supertest": "3.0.0"
    }
}
